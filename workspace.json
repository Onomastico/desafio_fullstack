{
  "version": 2,
  "cli": { "defaultCollection": "@nrwl/react" },
  "defaultProject": "front-carrito",
  "generators": {
    "@nrwl/react": {
      "application": { "style": "css", "linter": "eslint", "babel": true },
      "component": { "style": "css" },
      "library": { "style": "css", "linter": "eslint" }
    }
  },
  "projects": {
    "front-carrito": {
      "root": "apps/front-carrito",
      "sourceRoot": "apps/front-carrito/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/front-carrito",
            "index": "apps/front-carrito/src/index.html",
            "main": "apps/front-carrito/src/main.tsx",
            "polyfills": "apps/front-carrito/src/polyfills.ts",
            "tsConfig": "apps/front-carrito/tsconfig.app.json",
            "assets": [
              "apps/front-carrito/src/favicon.ico",
              "apps/front-carrito/src/assets"
            ],
            "styles": ["apps/front-carrito/src/styles.css"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/front-carrito/src/environments/environment.ts",
                  "with": "apps/front-carrito/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "front-carrito:build",
            "proxyConfig": "apps/front-carrito/proxy.conf.json"
          },
          "configurations": {
            "production": { "buildTarget": "front-carrito:build:production" }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/front-carrito/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/front-carrito"],
          "options": {
            "jestConfig": "apps/front-carrito/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "front-carrito-e2e": {
      "root": "apps/front-carrito-e2e",
      "sourceRoot": "apps/front-carrito-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/front-carrito-e2e/cypress.json",
            "tsConfig": "apps/front-carrito-e2e/tsconfig.e2e.json",
            "devServerTarget": "front-carrito:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "front-carrito:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/front-carrito-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "bff-carrito": {
      "root": "apps/bff-carrito",
      "sourceRoot": "apps/bff-carrito/src",
      "projectType": "application",
      "prefix": "bff-carrito",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/bff-carrito",
            "main": "apps/bff-carrito/src/main.ts",
            "tsConfig": "apps/bff-carrito/tsconfig.app.json",
            "assets": ["apps/bff-carrito/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/bff-carrito/src/environments/environment.ts",
                  "with": "apps/bff-carrito/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": { "buildTarget": "bff-carrito:build" }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["apps/bff-carrito/**/*.ts"] }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/bff-carrito"],
          "options": {
            "jestConfig": "apps/bff-carrito/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "ms-carrito": {
      "root": "apps/ms-carrito",
      "sourceRoot": "apps/ms-carrito/src",
      "projectType": "application",
      "prefix": "ms-carrito",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/ms-carrito",
            "main": "apps/ms-carrito/src/main.ts",
            "tsConfig": "apps/ms-carrito/tsconfig.app.json",
            "assets": ["apps/ms-carrito/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/ms-carrito/src/environments/environment.ts",
                  "with": "apps/ms-carrito/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": { "buildTarget": "ms-carrito:build" }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["apps/ms-carrito/**/*.ts"] }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/ms-carrito"],
          "options": {
            "jestConfig": "apps/ms-carrito/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "api-interface": {
      "root": "libs/api-interface",
      "sourceRoot": "libs/api-interface/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["libs/api-interface/**/*.ts"] }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/api-interface"],
          "options": {
            "jestConfig": "libs/api-interface/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
